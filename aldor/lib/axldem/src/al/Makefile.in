@SET_MAKE@
VPATH = @srcdir@

# For AM_V_*
AM_DEFAULT_VERBOSITY = @AM_DEFAULT_VERBOSITY@

builddir	:= @builddir@
abs_builddir	:= @abs_builddir@
top_builddir	:= @top_builddir@
abs_top_builddir:= @abs_top_builddir@
srcdir		:= @srcdir@
abs_srcdir	:= @abs_srcdir@
top_srcdir	:= @top_srcdir@
abs_top_srcdir	:= @abs_top_srcdir@
abs_libdir	:= $(abs_top_builddir)/lib/axldem/src/al
subdir		:= $(subst $(abs_top_builddir)/,,$(abs_builddir))

# Build starts here
library =		\
	dirprod		\
	gb		\
	ibits		\
	lmdict		\
	matopdom	\
	matrix		\
	nni		\
	poly		\
	poly3		\
	polycat		\
	prime		\
	quanc8		\
	random		\
	spf		\
	vector

axllibincdir	:= $(top_srcdir)/lib/axllib/include
axlliblibdir	:= $(top_builddir)/lib/axllib/src

libraryname	:= axldem

AXLFLAGS	:= -Q8
AXLFLAGS	+= -Y $(axlliblibdir) -I $(axllibincdir) -laxllib

include $(top_srcdir)/lib/buildlib.mk
