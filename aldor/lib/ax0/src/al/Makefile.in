axlibrary =		\
	basecliq	\
	aggcat	\
	any	\
	array1	\
	array2	\
	float	\
	fr	\
	fraction	\
	fspace	\
	indexedp	\
	integer	\
	kl	\
	list	\
	matcat	\
	matrix	\
	misc	\
	mkfunc	\
	multpoly	\
	op	\
	patmatch1	\
	patmatch2	\
	pattern	\
	poly	\
	polycat	\
	seg	\
	sex	\
	sf	\
	si	\
	stream	\
	symbol	\
	variable	\
	vector	\
	equation1	\
	equation2	\
	#

axllibincdir	:= $(top_srcdir)/lib/axllib/include

libraryname	:= ax0
Libraryname	:= Axiom

AXLFLAGS	:= -Q8 -Qinline-all -Wcheck -Wno-fatal
AXLFLAGS	+= -I $(axllibincdir)

bytecode_only	:= true

include $(top_srcdir)/lib/buildlib.mk
